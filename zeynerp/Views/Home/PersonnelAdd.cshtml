                                            @Html.TextBoxFor(x => x.RelativeName, new { id = "inputRelativeName", @class = "form-control", type = "text", required = "required" })
                                        </div>
                                        <div class="col-lg-6">
                                            <label for="inputRelativeSurname">Yakını Soyad</label>
                                            @Html.TextBoxFor(x => x.RelativeSurname, new { id = "inputRelativeSurname", @class = "form-control", type = "text", required = "required" })
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-xl-5 mr-auto">
                                        <label for="inputRelativePhone">Yakını Telefon</label>
                                        <div class="input-group">
                                            <span class="input-group-prepend">
                                                <span class="input-group-text">
                                                    <i class="fas fa-phone"></i>
                                                </span>
                                            </span>
                                            @Html.TextBoxFor(x => x.RelativePhone, new { id = "inputRelativePhone", @class = "form-control", type = "text", placeholder = "(123) 123-1234", required = "required" })
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group row align-items-center">
                                    <!-- <label class="col-lg-5 col-xl-3 control-label text-lg-right mb-0">First Name</label> -->
                                    <div class="col-lg-12 col-xl-10 ml-auto mr-auto">
                                        <label for="inputAddress">Adres</label>
                                        @Html.TextAreaFor(x => x.Address, new { id = "inputAddress", @class = "form-control", rows = "3", maxlength = "140", required = "required" })
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
        </div>
