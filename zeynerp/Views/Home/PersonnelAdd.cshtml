        <div class="row">
            <div class="col">
                <section class="card card-modern card-big-info">
                    <div class="card-body">
                        <div class="row">
                            <div class="col-lg-2-5 col-xl-1-5">
                                <div class="thumb-info mb-3">
                                    <img src="~/Assets/img/!logged-user.jpg" class="rounded img-fluid" alt="John Doe">
                                    <div class="thumb-info-edit">
                                        <label for="file-input">
                                            <i class="el el-edit"></i>
                                        </label>
                                        <input id="file-input" type="file" name="imageFile" onchange="this.form.submit()" style="display: none;" />
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-3-5 col-xl-4-5">
                                <div class="form-group row align-items-center">
                                    <!-- <label class="col-lg-5 col-xl-3 control-label text-lg-right mb-0">First Name</label> -->
                                    <div class="col-lg-6 col-xl-5 mb-3 mb-lg-0 ml-auto">
                                        <label for="inputFirstName">Ad</label>
                                        @Html.TextBoxFor(x => x.Name, new { id = "inputFirstName", @class = "form-control", type = "text", required = "required" })
                                    </div>
                                    <div class="col-lg-6 col-xl-5 mr-auto">
                                        <label for="inputLastName">Soyad</label>
                                        @Html.TextBoxFor(x => x.Surname, new { id = "inputLastName", @class = "form-control", type = "text", required = "required" })
                                    </div>
                                </div>
                                <div class="form-group row align-items-center">
                                    <!-- <label class="col-lg-5 col-xl-3 control-label text-lg-right mb-0">First Name</label> -->
                                    <div class="col-lg-6 col-xl-5 mb-3 mb-lg-0 ml-auto">
                                        <label for="inputFirstName">Doğum Yeri</label>
                                        @*<select data-plugin-selectTwo class="form-control populate">*@   
                                        @Html.DropDownListFor(model => model.PlaceOfBirth, new SelectList(ViewBag.Cities, "Value", "Text"), new { @class = "form-control populate" })
                                    </div>
                                    <div class="col-lg-6 col-xl-5 mr-auto">
                                        <label for="inputLastName">Doğum Tarihi</label>
                                        <div class="input-group">
                                            <span class="input-group-prepend">
                                                <span class="input-group-text">
                                                    <i class="fas fa-calendar-alt"></i>
                                                </span>
                                            </span>
                                            @*<input id="date" data-plugin-masked-input data-input-mask="99/99/9999" placeholder="__/__/____" class="form-control">*@
                                            @Html.TextBoxFor(x => x.Birthday, new { id = "date", @class = "form-control", type = "date", required = "required" })
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group row align-items-center">
                                    <!-- <label class="col-lg-5 col-xl-3 control-label text-lg-right mb-0">First Name</label> -->
                                    <div class="col-lg-6 col-xl-5 mb-3 mb-lg-0 ml-auto">
                                        <label for="inputFirstName">Vatandaşlık Numarası</label>
                                        @Html.TextBoxFor(x => x.TC_No, new { id = "inputTcNo", @class = "form-control", type = "text", required = "required" })
                                    </div>
                                    <div class="col-lg-6 col-xl-5 mr-auto">
                                        <label for="inputLastName">SGK Numarası</label>
                                        @Html.TextBoxFor(x => x.SGK_No, new { id = "inputSgkNo", @class = "form-control", type = "text", required = "required" })
                                    </div>
                                </div>
