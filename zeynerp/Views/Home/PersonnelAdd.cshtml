                                        <div class="input-group">
                                            <span class="input-group-prepend">
                                                <span class="input-group-text">
                                                    <i class="fas fa-phone"></i>
                                                </span>
                                            </span>
                                            @Html.TextBoxFor(x => x.Phone, new { id = "phone", @class = "form-control", type = "text", placeholder = "(123) 123-1234", required = "required" })
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-xl-5 row mr-auto p-0 m-0">
                                        <div class="col-lg-4 mb-3 mb-lg-0">
                                            <label for="inputVehicle">Araç</label>
                                            @Html.DropDownListFor(model => model.Vehicle, new SelectList(ViewBag.AvailableOrUnavailable, "Value", "Text"), new { @class = "form-control populate" })
                                        </div>
                                        <div class="col-lg-8">
                                            <label for="inputVehicleLicensePlate">Plaka</label>
                                            @Html.TextBoxFor(x => x.VehicleLicensePlate, new { id = "inputVehicleLicensePlate", @class = "form-control", type = "text" })
                                        </div>
                                    </div>
                                </div>
