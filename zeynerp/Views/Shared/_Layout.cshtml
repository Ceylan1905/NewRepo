                            {
                                var profileInfoes = Session["employee"] as Employee;
                                <span class="name">@profileInfoes.Name @profileInfoes.Surname</span>
                                <span class="role"> @profileInfoes.CompanyId</span>
                            }
                        </div>
                        <i class="fa custom-caret"></i>
