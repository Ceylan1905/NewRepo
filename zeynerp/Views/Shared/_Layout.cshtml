                </div>
            </div>

            <!-- start: search & user box -->
            <div class="header-right">
                <span class="separator"> </span>
                @{var employee = Session["employee"] as Employee; }
                @if (Session["employee"] != null && employee.IsAdmin == true)
                {
                    <div id="remainder" class="userbox">
                        Bakiye :

                        <div id="bakiyem" style="display:inline-block;">
                            @{PaymentManager payment = new PaymentManager();
                                var remainder = payment.GetRemainder(employee);
                                if (remainder == null)
                                {
                                    remainder = 0;

                                }
                                @remainder}
